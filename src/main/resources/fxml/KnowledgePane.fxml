<Accordion xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.TrackrMan.KnowledgeViewController" fx:id="accord">
   <panes>
      <TitledPane expanded="true" animated="true" text="Types of Parcel" fx:id="firstPane">
         <BorderPane>
            <top>
               <AnchorPane BorderPane.alignment="CENTER">
                  <styleClass>
                     <String fx:value="kno1"/>
                     <String fx:value="header"/>
                  </styleClass>
                  <HBox alignment="CENTER" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                        AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="0.0">
                     <ImageView fitWidth="600.0" pickOnBounds="true" preserveRatio="true"
                                AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <Image url="@../img/knowledge/kn_1.png"/>
                     </ImageView>
                  </HBox>
               </AnchorPane>
            </top>
            <center>
               <AnchorPane prefHeight="200.0" prefWidth="200.0" stylesheets="@../img/knowledge/kn_1.png"
