                  <styleClass>
                     <String fx:value="kno1"/>
                     <String fx:value="body"/>
                  </styleClass>
                  <HBox alignment="CENTER" prefHeight="201.0" prefWidth="602.0" AnchorPane.bottomAnchor="0.0"
                        AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <ImageView fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
                        <Image url="@../img/knowledge/kn_2.png"/>
                     </ImageView>
                  </HBox>
               </AnchorPane>
            </center>
         </BorderPane>
      </TitledPane>
      <TitledPane expanded="false" animated="true" text="FAQ">
         <BorderPane prefHeight="200.0" prefWidth="200.0">
            <top>
               <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
